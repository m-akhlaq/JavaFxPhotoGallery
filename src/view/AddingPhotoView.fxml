<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="350" prefWidth="300" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.AddingPhotoController">
   <children>
     <TextField fx:id="captionField" layoutX="74.0" layoutY="52.0" prefHeight="40.0" prefWidth="178.0" />
     <Button fx:id="addButton" layoutX="136.0" layoutY="230.0" mnemonicParsing="false" onAction="#addPhoto" prefHeight="25.0" prefWidth="55.0" text="Add" />
     <Button fx:id="filePickerButton" layoutX="136.0" layoutY="182.0" mnemonicParsing="false" onAction="#openFilePicker" text="Browse" />
      <Label layoutX="14.0" layoutY="64.0" text="Caption:" />
      <TextField  fx:id="tagField" layoutX="75.0" layoutY="107.0" prefHeight="40.0" prefWidth="178.0" />
      <Label layoutX="22.0" layoutY="119.0" text="Tags:" />
      <Label fx:id="filePickerLabel" layoutX="218.0" layoutY="186.0" text="Label" visible="false" />
   </children>
</AnchorPane>
