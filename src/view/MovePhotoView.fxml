<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="213.0" prefWidth="306.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.MovePhotoController">
   <children>
      <Button layoutX="103.0" layoutY="171.0" mnemonicParsing="false" prefHeight="28.0" prefWidth="101.0" text="Move" onAction="#movePhoto" />
      <ComboBox fx:id="albumComboBox" layoutX="142.0" layoutY="107.0" prefWidth="150.0" />
      <Label layoutX="14.0" layoutY="111.0" text="Please select an album:" />
      <RadioButton fx:id="copyRadioButton" layoutX="14.0" layoutY="44.0" mnemonicParsing="false" selected="true" text="Copy and Move">
         <toggleGroup>
            <ToggleGroup fx:id="group" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="cutRadioButton" layoutX="196.0" layoutY="44.0" mnemonicParsing="false" text="Cut and Move" toggleGroup="$group" />
   </children>
</AnchorPane>
