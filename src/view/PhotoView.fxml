<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400" prefWidth="600" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.PhotoViewController">
   <children>
      <ListView fx:id="photoView" layoutX="5.0" layoutY="53.0" onMouseClicked="#doubleClickPhoto" prefHeight="342.0" prefWidth="589.0" />
      <Button fx:id="addPhotoButton" layoutX="105.0" layoutY="14.0" mnemonicParsing="false" onAction="#addPhoto" prefHeight="25.0" prefWidth="85.0" text="Add" />
      <Button fx:id="searchPhotoButton" layoutX="529.0" layoutY="14.0" mnemonicParsing="false" onAction="#openSearch" prefHeight="25.0" prefWidth="65.0" text="Search" />
      <Button fx:id="backButton" onAction="#goBack" layoutX="5.0" layoutY="14.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="65.0" text="Back" />
      <Button fx:id="deleteButton" onAction="#deletePhoto" layoutX="201.0" layoutY="14.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="85.0" text="Delete Selected">
         <font>
            <Font size="10.0" />
         </font>
      </Button>
      <Button fx:id="editPhotoButton" onAction="#editPhoto" layoutX="293.0" layoutY="14.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="85.0" text="Edit Selected" />
      <Button fx:id="slideshowButton" onAction="#startSlideshow" layoutX="388.0" layoutY="14.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="85.0" text="SlideShow" />
   </children>
</AnchorPane>
