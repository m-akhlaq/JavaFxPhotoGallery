<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400" prefWidth="626.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.PhotoViewController">
   <children>
      <ListView fx:id="photoView" layoutX="5.0" layoutY="53.0" onMouseClicked="#doubleClickPhoto" prefHeight="342.0" prefWidth="519.0" />
      <Button fx:id="addPhotoButton" layoutX="5.0" layoutY="14.0" mnemonicParsing="false" onAction="#addPhoto" prefHeight="25.0" prefWidth="85.0" text="Add" />
      <Button fx:id="searchPhotoButton" layoutX="529.0" layoutY="168.0" mnemonicParsing="false" onAction="#openSearch" prefHeight="25.0" prefWidth="86.0" text="Search" />
      <Button fx:id="backButton" layoutX="539.0" layoutY="349.0" mnemonicParsing="false" onAction="#goBack" prefHeight="25.0" prefWidth="65.0" text="Back" />
      <Button fx:id="deleteButton" layoutX="271.0" layoutY="14.0" mnemonicParsing="false" onAction="#deletePhoto" prefHeight="25.0" prefWidth="85.0" text="Delete Selected">
         <font>
            <Font size="10.0" />
         </font>
      </Button>
      <Button fx:id="editPhotoButton" layoutX="529.0" layoutY="14.0" mnemonicParsing="false" onAction="#editPhoto" prefHeight="25.0" prefWidth="85.0" text="Edit Selected" />
      <Button fx:id="slideshowButton" layoutX="529.0" layoutY="233.0" mnemonicParsing="false" onAction="#startSlideshow" prefHeight="25.0" prefWidth="86.0" text="SlideShow" />
      <Button layoutX="529.0" layoutY="292.0" mnemonicParsing="false" onAction="#movePhoto" text="Move Photos" />
      <Button onAction="#openPhoto" layoutX="529.0" layoutY="108.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="86.0" text="Open Selected" AnchorPane.bottomAnchor="267.0" />
   </children>
</AnchorPane>
