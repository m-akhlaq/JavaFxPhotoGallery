<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="240.0" prefWidth="300" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.EditingPhotoController">
   <children>
     <TextField fx:id="captionField" layoutX="74.0" layoutY="52.0" prefHeight="40.0" prefWidth="178.0" />
     <Button fx:id="editButton" layoutX="130.0" layoutY="201.0" mnemonicParsing="false" onAction="#editPhoto" prefHeight="25.0" prefWidth="55.0" text="Edit" />
      <Label layoutX="14.0" layoutY="64.0" text="Caption:" />
      <TextField fx:id="tagField" layoutX="75.0" layoutY="107.0" prefHeight="40.0" prefWidth="178.0" visible="false" />
      <Label layoutX="83.0" layoutY="14.0" text="Edit your Photo Information" />
      <Button fx:id="addTagButton" layoutX="124.0" layoutY="108.0" mnemonicParsing="false" onAction="#addTag" text="Add Tags" />
      <Button fx:id="showDeleteButton" onAction="#showDeleteMenu" layoutX="117.0" layoutY="147.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="80.0" text="Delete Tags" />
      <ComboBox fx:id="tagComboBox" visible="false" layoutX="8.0" layoutY="147.0" prefHeight="25.0" prefWidth="148.0" />
      <Button fx:id="deleteTagButton" onAction="#deleteTag" visible="false" layoutX="172.0" layoutY="147.0" mnemonicParsing="false" text="Delete" />
      <Button fx:id="cancelButton" visible="false" onAction="#resetMenu" layoutX="227.0" layoutY="147.0" mnemonicParsing="false" text="Cancel" />
   </children>
</AnchorPane>
